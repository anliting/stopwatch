let version='0'
onactivate=e=>e.waitUntil((async()=>
    Promise.all((await caches.keys()).map(k=>
        k==version||caches.delete(k)
    ))
)())
onfetch=e=>e.respondWith((async()=>
    (await caches.open(version)).match(e.request)
)())
oninstall=e=>e.waitUntil((async()=>
    (await caches.open(version)).addAll([
        '.',
        'icon',
        'manifest',
        'mi.woff2',
    ])
)())
