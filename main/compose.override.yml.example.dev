services:
    app:
        build:
            context: stopwatch
            dockerfile: main/Dockerfile
    gateway:
        image: nginx:1.25.5
        ports:
            - '[::1]:443:443'
        volumes:
            - ./gatewayCnf:/etc/nginx/conf.d/default.conf
            - ./crt:/etc/ssl/certs/server.crt
            - ./key:/etc/ssl/certs/server.key
